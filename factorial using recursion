import java.util.*; 
public class Main{ 
    
    public static int fact(int n){
        if(n>0){
            return n*fact(n-1);
        }else{
            return 1;
        }
    }
    public static void main(String[] args){ 
        Scanner sc = new Scanner(System.in); 
      int n = sc.nextInt();
      
      int fac  = fact(n);
      
     System.out.println("Factorial is :"+fac);
    }
}
