import java.util.*; 
public class Main{ 
    public static void main(String[] args){ 
        Scanner sc = new Scanner(System.in); 
        String s = sc.nextLine();
        char[] c = s.toCharArray();
        System.out.println("Before "+(Arrays.toString(c)).replaceAll("[ ,\\[\\]]",""));
        Arrays.sort(c);
        System.out.println("After "+(Arrays.toString(c)).replaceAll("[ ,\\[\\]]",""));
    }
}
