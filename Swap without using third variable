import java.util.*;
public class Main{
    
    public static void main(String[] args){
        Scanner sc  =  new Scanner(System.in);
        int i=sc.nextInt();
        int j=sc.nextInt();
        System.out.println("Before swapping i="+i+"; j="+j);
        
        i=i+j;
        j=i-j;
        i=i-j;
        
        System.out.println("After swapping i="+i+"; j="+j);
        
    }
};
