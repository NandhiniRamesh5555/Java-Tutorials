import java.util.*; 
public class Main{ 
    public static void main(String[] args){ 
        Scanner sc = new Scanner(System.in); 
        int n = sc.nextInt();
        int[] inp = new int[n];
        for(int i=0;i<n;i++){
            inp[i]=sc.nextInt();
        }
        System.out.println("Before "+Arrays.toString(inp));
        Arrays.sort(inp);
        System.out.println("After "+Arrays.toString(inp));
    }
}
